{
  "name": "react-layered-chart",
  "version": "0.6.1",
  "description": "A high-performance canvas-based time series visualization in React.",
  "main": "lib/index.js",
  "typings": "typings/index.d.ts",
  "author": "Palantir Technologies",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/palantir/react-layered-chart.git"
  },
  "scripts": {
    "dev:js": "webpack-dev-server --inline --open",
    "dev:css": "stylus --watch --sourcemap --out build-dev/ styles/",
    "test": "multi=\"mocha-osx-reporter=- spec=-\" mocha --reporter mocha-multi",
    "build": "babel --source-maps true src/ --out-dir lib/ && stylus --sourcemap --out lib/ styles/index.styl",
    "prepublish": "npm test -- --reporter landing && npm run build",
    "postinstall": "tsd install"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-decorators-legacy",
      "lodash"
    ]
  },
  "devDependencies": {
    "babel": "^6.3.13",
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^2.0.1",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "memoizee": "^0.3.9",
    "mocha": "^2.3.4",
    "mocha-multi": "^0.7.2",
    "mocha-osx-reporter": "^0.1.2",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "should": "^8.0.2",
    "sinon": "^1.17.2",
    "stylus": "^0.53.0",
    "tsd": "^0.6.5",
    "typescript": "^1.8.10",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.1",
    "d3-ease": "^0.7.0",
    "d3-interpolate": "^0.7.0",
    "d3-scale": "^0.7.0",
    "lodash": "^4.0.0",
    "pure-render-decorator": "^1.0.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-debounced": "^0.2.0",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^1.0.3",
    "reselect": "^2.5.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^0.14.3",
    "react-dom": "^15.0.0 || ^0.14.3"
  },
  "keywords": [
    "timeline",
    "time",
    "series",
    "chart",
    "canvas",
    "react",
    "component"
  ]
}
